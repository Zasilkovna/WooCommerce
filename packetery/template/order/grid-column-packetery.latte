{varType \Packetery\Module\Order\Entity $order}
{varType string $nonce}
{varType string $orderSaveUrl}

<a class="packetery-order-inline-edit button" href="#"
   data-order-data="{= json_encode(
	   [
		   'id' => $order->getId(),
		   'packetery_weight' => $order->getPacketeryWeight(),
	   ]
   )}"
   data-nonce="{$nonce}"
   data-order-save-url="{$orderSaveUrl}"
>&#9998;</a>
