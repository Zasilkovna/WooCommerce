{
    "require-dev": {
        "wpify/scoper": "3.0.1",
        "woocommerce/woocommerce-sniffs": "^0.1"
    },
    "config": {
        "platform": {
            "php": "8.1"
        },
        "allow-plugins": {
            "wpify/scoper": true,
            "dealerdirect/phpcodesniffer-composer-installer": true
        }
    },
    "extra": {
        "wpify-scoper": {
            "prefix": "Packetery",
            "folder": "deps",
            "temp": "temp/scoper-works",
            "composerjson": "composer-deps.json",
            "composerlock": "composer-deps.lock"
        }
    }
}
